<div class="page-header">
  <h3><%= @user.username %>'s Likes</h3>
</div>
<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <dt>Likes</dt>
      <dd>
        <ul class="list-group">
          <% @user.likes.each do |like| %>
          <li class="list-group-item">
            <img img src="<%= like.photo.image%>" width="100">
            <%= like.photo.caption %>
          </li>
        <% end %>
        </ul>
      </dd>

    </dl>


  </div>
</div>
